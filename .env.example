# Amazon Seller Central Credentials
# WARNING: Keep these secure - never commit to git
AMAZON_SELLER_EMAIL=your-seller-email@example.com
AMAZON_SELLER_PASSWORD=your-secure-password

# Seller Central URLs
SELLER_CENTRAL_URL=https://sellercentral.amazon.com

# LQS Integration
LQS_API_URL=https://lqs.krell.works/api
LQS_THRESHOLD=70.0

# AWS S3 Configuration (for listing data)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
S3_BUCKET=acglogs
S3_PREFIX=listings/

# Experiment Settings
DEFAULT_EXPERIMENT_DURATION=28
DEFAULT_TRAFFIC_SPLIT=50

# Browser Automation Settings
HEADLESS_MODE=false
SCREENSHOT_ON_ERROR=true
